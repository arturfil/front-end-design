<div class="container spot-details">
  <h2> <%= spotsInfo.name %></h2>
  <img class="image-detail"src="<%= spotsInfo.image %>" alt="">
  <br>
  <br>
  <p> <%= spotsInfo.workout %></p>
  <p> <%= spotsInfo.location %></p>
  <p> <%= spotsInfo.rating %></p>
  <a class="btn btn-default" href="/spots/<%=spotsInfo._id%>/edit">Edit</a>
</div>


    <h3>My Google Maps Demo</h3>
    <div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: -25.363, lng: 131.044};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBMn0osJRWlzZFbKyGyOGSeAULr4vp3cRI&callback=initMap">
    </script>
